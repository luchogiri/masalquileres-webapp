    "multer": "^1.3.0",
    "node-ses": "^2.0.5",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "postcss-cssnext": "^2.6.0",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "react": "^15.1.0",
    "react-addons-create-fragment": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-animations": "^0.1.0",
    "react-bootstrap": "^0.30.7",
